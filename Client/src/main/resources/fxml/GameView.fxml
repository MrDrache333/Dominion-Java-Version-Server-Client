<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal">
    <children>
        <Button fx:id="logoutButton" layoutX="550.0" layoutY="376.0" mnemonicParsing="false"
                onAction="#onLogoutButtonPressed" text="Logout" translateX="-10.0" translateY="-10.0"
                GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1"
                GridPane.valignment="BOTTOM"/>
        <GridPane layoutX="123.0" layoutY="139.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <Pane fx:id="chatView" layoutY="78.0" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2"/>
        <Button fx:id="giveUpButton" layoutX="534.0" layoutY="78.0" mnemonicParsing="false"
                onAction="#onGiveUpButtonPressed" text="Aufgeben"/>
        <ListView fx:id="usersView" layoutX="402.0" layoutY="114.0" minWidth="100.0" prefHeight="200.0"
                  prefWidth="200.0" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"/>
    </children>
</AnchorPane>
