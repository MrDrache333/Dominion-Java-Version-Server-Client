<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorPane" prefHeight="280.0" prefWidth="420.0"
            xmlns="http://javafx.com/javafx/8.0.999-ea">
    <ImageView fx:id="background" fitHeight="330.0" fitWidth="430.0" layoutX="180.0" layoutY="227.0" pickOnBounds="true"
               preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Image url="@../images/burgHohenzollern.jpg"/>
    </ImageView>
    <ImageView fx:id="bannerDrawing" fitWidth="300.0" layoutX="30.0" layoutY="135.0" preserveRatio="true" rotate="-2.5">
        <Image url="@../images/bannerDrawing.png"/>
    </ImageView>
    <Text fx:id="bannerText" layoutX="90" layoutY="196.0" text="Du hast gewonnen" wrappingWidth="200.0"
          style="-fx-font-size: 16">
    </Text>
    <Text fx:id="bannerTextSmall" layoutX="90" layoutY="216.0" text="Herzlichen Glückwunsch!" wrappingWidth="200.0"
          style="-fx-font-size: 12">
    </Text>
    <ImageView fx:id="trophy" fitWidth="35.0" layoutX="233.0" layoutY="175.0" preserveRatio="true">
        <Image url="@../images/trophy.png"/>
    </ImageView>
    <HBox alignment="BASELINE_RIGHT" layoutX="270.0" layoutY="250.0" prefHeight="20" prefWidth="150" spacing="7.0">
        <children>
            <Button layoutX="280.0" layoutY="250.0" mnemonicParsing="false" onAction="#onAgainButtonPressed"
                    text="nochmal"/>
            <Button layoutX="355.0" layoutY="250.0" mnemonicParsing="false" onAction="#onReturnButtonPressed"
                    text="aufhören"/>
        </children>
    </HBox>
</AnchorPane>
